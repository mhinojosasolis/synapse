					},
					"collapsed": false
				},
				"source": [
					"%%pyspark\r\n",
					"\r\n",
					"# definimos las variables para los archivos de entrada y salida\r\n",
					"nombre_archivo_origen = 'abfss://devraw@devmycdlake.dfs.core.windows.net/minecare/history/REPORTING_TRENDEVENTDETAILS_HIS.parquet'\r\n",
					"nombre_archivo_destino = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataHistorica/minecare/REPORTING_TRENDEVENTDETAILS_HIS.parquet'"
				],
				"attachments": null,
				"execution_count": 1
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# traemos la tabla histórica del origen\r\n",
					"df = spark.read.load(nombre_archivo_origen, format='parquet')\r\n",
					"display(df.limit(10))"
				],
				"attachments": null,
				"execution_count": 10
			},
