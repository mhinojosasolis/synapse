				"source": [
					"### Notebook para crear el histórico con checksum de alguna tabla"
				],
				"attachments": null
			},
			{
				"cell_type": "code",
				"metadata": {
					"microsoft": {
						"language": "python"
					},
					"collapsed": false
				},
				"source": [
					"%%pyspark\r\n",
					"\r\n",
					"# definimos las variables para los archivos de entrada y salida\r\n",
					"nombre_archivo_origen = 'abfss://devraw@devmycdlake.dfs.core.windows.net/minecare/history/AGGREGATEDOEMPARAMVALUE_HIS.parquet'\r\n",
					"nombre_archivo_destino = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataHistorica/minecare/AGGREGATEDOEMPARAMVALUE_HIS.parquet'"
				],
				"attachments": null,
				"execution_count": 2
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# traemos la tabla histórica del origen\r\n",
					"df = spark.read.load(nombre_archivo_origen, format='parquet')\r\n",
					"display(df.limit(10))"
				],
				"attachments": null,
				"execution_count": 3
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
					"df.count()"
				],
				"attachments": null,
				"execution_count": 4
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
					"df.columns"
				],
				"attachments": null,
				"execution_count": 5
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
					"# traigo todas las columnas y le quito las columnas de la fecha de extracción y el checksum, que son determinísticos\r\n",
					"# se captura el error en el caso de no existir la columna a remover.\r\n",
					"cols = df.columns\r\n",
					"\r\n",
					"try:\r\n",
					"  cols.remove('ExtractDate')\r\n",
					"except:\r\n",
					"  print(\"la columna ExtractDate no existe\")\r\n",
					"\r\n",
					"try:\r\n",
					"  cols.remove('checksum')\r\n",
					"except:\r\n",
					"  print(\"la columna checksum no existe\")\r\n",
					"\r\n",
					"cols"
				],
				"attachments": null,
				"execution_count": 6
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# concateno todas las columnas y luego calculo el checksum con sha1\r\n",
					"from pyspark.sql.functions import concat_ws, sha1, col, to_date, date_format\r\n",
					"\r\n",
					"df = df.withColumn(\"checksum\", sha1(concat_ws('-',*cols)))\\\r\n",
					"       .withColumn(\"PartitionDate\", concat_ws('-', date_format(col('ReadTime'), \"yyyy\"), date_format(col('ReadTime'), \"MM\")))        \r\n",
					"display(df.limit(10))"
				],
				"attachments": null,
				"execution_count": 7
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
