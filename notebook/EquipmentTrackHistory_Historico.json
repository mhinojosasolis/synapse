				"source": [
					"### Notebook para carga de datos incrementales de tabla de origen EquipmentTrackHistory de Minecare"
				],
				"attachments": null
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"microsoft": {
						"language": "python"
					},
					"collapsed": true
				},
				"source": [
					"%%pyspark\r\n",
					"\r\n",
					"nombre_archivo_origen = 'abfss://devraw@devmycdlake.dfs.core.windows.net/minecare/temp/MINECARE_EQUIPMENTTRACKHISTORY_TEMP.parquet'\r\n",
					"nombre_archivo_historico = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataHistorica/minecare/EQUIPMENTTRACKHISTORY_HISTORICO.parquet'"
				],
				"attachments": null,
				"execution_count": 42
			},
			{
				"cell_type": "code",
				"metadata": {
					"collapsed": false
				},
				"source": [
					"# traemos los datos de origen incrementales\r\n",
					"df = spark.read.load(nombre_archivo_origen, format='parquet')\r\n",
					"display(df.limit(10))"
				],
				"attachments": null,
				"execution_count": 43
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
					"df.count()"
				],
				"attachments": null,
				"execution_count": 44
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
					"df.columns"
				],
				"attachments": null,
				"execution_count": 45
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
					"# Para calculo el checksum, armo una lista con la coleccion de columnas y saco las que no necesito, \r\n",
					"# para no tener que armar la lista manualmente\r\n",
					"# traigo todas las columnas y le quito las columnas de la fecha de extracción y el checksum, que son determinísticos\r\n",
					"# se captura el error en el caso de no existir la columna a remover.\r\n",
					"# NO ESTOY ELIMINANDO LAS COLUMNAS DE LOS DATAFRAMES\r\n",
					"cols = df.columns\r\n",
					"\r\n",
					"try:\r\n",
					"  cols.remove('ExtractDate')\r\n",
					"except:\r\n",
					"  print(\"la columna ExtractDate no existe\")\r\n",
					"\r\n",
					"try:\r\n",
					"  cols.remove('checksum')\r\n",
					"except:\r\n",
					"  print(\"la columna checksum no existe\")\r\n",
					"\r\n",
					"cols"
				],
				"attachments": null,
				"execution_count": 46
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# concateno todas las columnas seleccionadas y luego calculo el checksum con sha1\r\n",
					"# se lo agregamos al df original\r\n",
					"from pyspark.sql.functions import concat_ws, sha1, col, to_date, date_format\r\n",
					"\r\n",
					"df = df.withColumn(\"checksum\", sha1(concat_ws('-',*cols)))\\\r\n",
					"        .withColumn(\"PartitionDate\", concat_ws('-', date_format(col('UpdateTime'), \"yyyy\"), date_format(col('UpdateTime'), \"MM\")))\r\n",
					"display(df.limit(10))"
				],
				"attachments": null,
				"execution_count": 48
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
