				"display_name": "python"
			},
			"language_info": {
				"name": "python"
			}
		},
		"cells": [
			{
				"cell_type": "code",
				"source": [
					"container_raw = 'devraw'\n",
					"coitainer_proc = 'devproc'\n",
					"blobstorage = 'devmycdlake.dfs.core.windows.net'\n",
					"blobstorage_raw_path = 'abfss://devraw@'+blobstorage\n",
					"blobstorage_proc_path = 'abfss://devproc@'+blobstorage"
				],
				"attachments": null,
				"execution_count": null
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
					"print('Variables cargadas correctamente')"
				],
				"attachments": null,
				"execution_count": null
			}
		]
	}
}