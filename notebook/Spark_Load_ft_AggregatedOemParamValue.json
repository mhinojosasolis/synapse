				},
				"source": [
					"%%pyspark\r\n",
					"# Archivo final para la fact table\r\n",
					"nombre_archivo_ft = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataMultidim/minecare/ft_AggregatedOemParamValue.parquet'\r\n",
					"# nombre_archivo_historico = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataHistorica/minecare/EQUIPMENTTRACKHISTORY_HISTORICO.parquet'\r\n",
					"nombre_archivo_historico = 'abfss://devraw@devmycdlake.dfs.core.windows.net/minecare/history/AGGREGATEDOEMPARAMVALUE_HIS.parquet'\r\n",
					""
				],
				"attachments": null,
				"execution_count": 1
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# traemos los datos de origen incrementales\r\n",
					"df = spark.read.load(nombre_archivo_historico, format='parquet')\r\n",
					"display(df.limit(10))\r\n",
					""
				],
				"attachments": null,
				"execution_count": 2
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# Se renombran las columnas y se generan los id dimensionales\r\n",
					"from pyspark.sql.functions import concat_ws, sha1, col, to_date, date_format,to_utc_timestamp, current_timestamp\r\n",
					"\r\n",
					"df = df.withColumnRenamed(\"Id\",\"id_AggOemParamValue\").withColumnRenamed(\"EquipmentId\",\"id_Equipment\").withColumn(\"BatchDate\", to_utc_timestamp(current_timestamp(), \"UTC\"))\r\n",
					"df = df.select(\"id_AggOemParamValue\",\"SiteId\",\"SetId\",\"id_Equipment\",\"ConsumerId\",\"OemParamTypeId\",\"ReadTime\",\"MinValue\",\"MaxValue\",\"MeanValue\",\"MedianValue\",\"StdDevValue\",\"NumberOfSamples\")\r\n",
					"display(df.limit(10))"
				],
				"attachments": null,
				"execution_count": 6
			},
