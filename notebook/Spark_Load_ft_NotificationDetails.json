					"\r\n",
					"df = df.withColumn(\"id_Notification\", sha1(concat_ws('-',*cols))).withColumnRenamed(\"ModelId\",\"id_Model\").withColumnRenamed(\"EquipmentId\",\"id_Equipment\").withColumnRenamed(\"OemEventId\",\"id_OemEvent\").withColumnRenamed(\"OperatorId\",\"id_Operator\").withColumnRenamed(\"InterfaceId\",\"id_interface\").withColumn(\"BatchDate\", to_utc_timestamp(current_timestamp(), \"UTC\"))\r\n",
