					"microsoft": {
						"language": "python"
					},
					"collapsed": true
				},
				"source": [
					"%%pyspark\r\n",
					"\r\n",
					"nombre_archivo_ft = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataMultidim/minecare/ft_OemEventSnapshotValue.parquet'\r\n",
					"# nombre_archivo_historico = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataHistorica/minecare/EQUIPMENTTRACKHISTORY_HISTORICO.parquet'\r\n",
					"nombre_archivo_historico = 'abfss://devraw@devmycdlake.dfs.core.windows.net/minecare/history/OEMEVENTSNAPSHOTVALUE_HIS.parquet'\r\n",
					"# Carga de dimensiones relacionadas con la fact table para leer los id dimensionales\r\n",
					"#dm_Equipment = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataMultidim/minecare/dm_Equipment.parquet/'\r\n",
					"#dm_Location = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataMultidim/minecare/dm_Location.parquet/'\r\n",
					"#dm_Operator = ''\r\n",
					"#dm_MaterilaType = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataMultidim/minecare/dm_MaterialType.parquet/'\r\n",
					"#dm_Action = 'abfss://devproc@devmycdlake.dfs.core.windows.net/DataMultidim/minecare/dm_Action.parquet/'"
				],
				"attachments": null,
				"execution_count": 2
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# traemos los datos de origen incrementales\r\n",
					"df = spark.read.load(nombre_archivo_historico, format='parquet')\r\n",
					"display(df.limit(10))\r\n",
					""
				],
				"attachments": null,
				"execution_count": 3
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# traigo todas las columnas y le quito las columnas de la fecha de extracción y el checksum, que son determinísticos\r\n",
					"# se captura el error en el caso de no existir la columna a remover.\r\n",
					"cols = df.columns\r\n",
					"\r\n",
					"try:\r\n",
					"  cols.remove('ExtractDate')\r\n",
					"except:\r\n",
					"  print(\"la columna ExtractDate no existe\")\r\n",
					"\r\n",
					"try:\r\n",
					"  cols.remove('checksum')\r\n",
					"except:\r\n",
					"  print(\"la columna checksum no existe\")\r\n",
					""
				],
				"attachments": null,
				"execution_count": 4
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"# Se renombran las columnas y se generan los id dimensioanles\r\n",
					"from pyspark.sql.functions import concat_ws, sha1, col, to_date, date_format,to_utc_timestamp, current_timestamp\r\n",
					"\r\n",
					"df = df.withColumnRenamed(\"Id\",\"id_OemEventSnap\").withColumnRenamed(\"EquipmentId\",\"id_Equipment\").withColumn(\"BatchDate\", to_utc_timestamp(current_timestamp(), \"UTC\"))\r\n",
					"df = df.select(col(\"id_OemEventSnap\"),col(\"SiteId\"),col(\"id_Equipment\"),col(\"OemEventTypeId\"),col(\"EventTime\"),col(\"FloatValue\"),col(\"StringValue\"))\r\n",
					"display(df.limit(10))"
				],
				"attachments": null,
				"execution_count": 5
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"#### Genero la dimensión final a grabar en el data lake"
				],
				"attachments": null
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": true
				},
				"source": [
					"# grabo la versión final de la dimension\r\n",
					"df.write.mode(\"overwrite\").parquet(nombre_archivo_ft)"
				],
				"attachments": null,
